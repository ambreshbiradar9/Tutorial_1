program N_R
implicit none
real, parameter::tol =1e-5
integer::i
real::x0,x1,fun,dfun
read*, x0
i=1
10 x1= x0-(fun(x0)/dfun(x0))
if(abs((x1- x0)/x1)< tol)then
print*,"root is ", x1 ,"no. of iteration=", i
else
x0=x1
i=i+1
goto 10
endif
end
real function fun(x)
real::x
end
real function dfun(x)
real::x
fd=3*x**2-1
end
